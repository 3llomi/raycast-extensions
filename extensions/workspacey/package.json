{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "workspacey",
  "title": "Workspacey",
  "description": "Create and manage your workspaces, open multiple folders at once.",
  "icon": "icon.png",
  "author": "3llomi",
  "categories": [
    "Productivity",
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "create-workspace",
      "title": "Create Workspace",
      "subtitle": "Create New Workspace",
      "description": "Create New Workspace",
      "mode": "view"
    },
    {
      "name": "show-workspaces",
      "title": "Start Workspace",
      "subtitle": "Start Workspace",
      "description": "Start Workspace",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.69.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}